{
  "title": "2017 Day Day Up",
  "description": "用于记录 2017 年中遇到的各种零碎知识",
  "author": "new4",
  "gitbook": ">=3.2.2",
  "plugins": [
    "katex",
    "simple-page-toc",
    "-highlight",
    "prism",
    "prism-themes",
    "github",
    "splitter",
    "expandable-chapters-small"
  ],
  "pluginsConfig": {
    "simple-page-toc": {
      "maxDepth": 3,
      "skipFirstH1": true
    },
    "prism": {
      "css": [
        "prism-themes/themes/prism-xonokai.css"
      ]
    },
    "github": {
      "url": "https://github.com/new4"
    }
  },
  "styles": {
    "website": "styles/website.css"
  }
}
